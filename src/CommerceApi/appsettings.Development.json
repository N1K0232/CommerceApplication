    "Serilog": {
        "Using": [ "Serilog.Sinks.File", "CommerceApi.Logging" ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.AspNetCore.Identity": "Error",
                "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
                "System": "Warning"
